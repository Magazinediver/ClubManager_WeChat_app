<view class="container">
  <view class="id-name-group">
    <van-cell-group>
      <van-field
        value="{{ username }}"
        required
        clearable
        label="真实姓名"
        placeholder="请输入真实姓名"
      />

      <van-field
        value="{{ password }}"
        clearable
        type="number"
        label="学号"
        placeholder="请输入学号"
        required
        border="{{ false }}"
      />
    </van-cell-group>
  </view>
  
</view>
