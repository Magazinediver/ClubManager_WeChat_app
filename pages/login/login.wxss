.logo_logo {
  display: flex;
  border-radius: 40rpx;
  border: 1rpx solid #f2f2f2;
  justify-content: center;
  width: 150rpx;
  height: 150rpx;
  margin: 0 auto;
  margin-top: 150rpx;
}
.logo_text {
  display: flex;
  justify-content: center;
  margin: 0 auto;
  margin-top: 15rpx;
  font-size: 36rpx;
  font-weight: 500;
}
.login_button {
  margin: 0 auto;
  margin-top: 225rpx;
  border-radius: 15rpx;
  background-color: #ffc609;
  font-size: 30rpx;
  letter-spacing: 2rpx;
  line-height: 90rpx;
  font-weight: 500;
  text-align: center;
}
.register {
  width: 100%;
  height: 30rpx;
  margin-top: 40rpx;
  font-size: 28rpx;
  letter-spacing: 2rpx;
  line-height: 30rpx;
  font-weight: light;
  text-align: center;
  color: #647896;
}
.login_contract {
  width: 100%;
  height: 30rpx;
  margin-top: 540rpx;
  text-align: center;
}
.login_contract .login_con_1 {
  font-size: 24rpx;
